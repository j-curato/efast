{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Project Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4.11",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "dmstr/yii2-adminlte-asset": "^2.1",
    "2amigos/yii2-chartjs-widget": "~2.0",
    "kartik-v/yii2-widgets": "*",
    "kartik-v/yii2-grid": "dev-master",
    "picqer/php-barcode-generator": "^0.3.0",
    "kartik-v/yii2-money": "dev-master",
    "kartik-v/yii2-mpdf": "dev-master",
    "kartik-v/yii2-widget-datepicker": "dev-master",
    "kartik-v/yii2-field-range": "*",
    "wbraganca/yii2-dynamicform": "dev-master",
    "kartik-v/yii2-number": "dev-master",
    "aryelds/yii2-sweet-alert": "@dev",
    "phpoffice/phpspreadsheet": "*",
    "philippfrenzel/yii2fullcalendar": "*",
    "edofre/yii2-fullcalendar": "V1.0.11",
    "kartik-v/yii2-export": "@dev",
    "2amigos/qrcode-library": "^2.0",
    "2amigos/yii2-google-maps-library": "*",
    "vilochane/yii2-barcode-generator": "dev-master",
    "softark/yii2-mb-captcha": "dev-master",
    "razonyang/yii2-rate-limiter": "^1.0",
    "kartik-v/yii2-icons": "@dev",
    "yiisoft/yii2-symfonymailer": "^3.0.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.1.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0",
    "symfony/browser-kit": ">=2.7 <=4.2.4"

  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "platform-check": false,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "conflict": {
    "bower-asset/fullcalendar": "^4.3.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
